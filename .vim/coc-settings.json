{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "Markdown","javascript"
                                              ,"typescript","html","vue"],
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b", "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "typescript.showUnused": true,
  "javascript.showUnused": true,
  "eslint.autoFixOnSave": true,
  "eslint.run": "onType",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "vetur.validation.template": true,
  "vetur.validation.script": true,
  "vetur.format.defaultFormatterOptions":{
    "prettier": {
      "singelQuote":true,
      "semi":false,
      "trailingComma":"es5"
    }
  },
  "vetur.trace.server": "verbose",
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "shadow-cljs.edn", "build.boot", ".git"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": { } ,
      "disableDiagnostics":true,
      "disableCompletion":true
    }
  }
}
